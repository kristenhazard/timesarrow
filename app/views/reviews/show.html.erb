<% title "Review" %>

<p>
  <strong>Item:</strong>
  <%=h @review.item_id %>
</p>
<p>
  <strong>User:</strong>
  <%=h @review.user_id %>
</p>
<p>
  <strong>Content:</strong>
  <%=h @review.content %>
</p>

<p>
  <%= link_to "Edit", edit_review_path(@review) %> |
  <%= link_to "Destroy", @review, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", reviews_path %>
</p>
